   "source": [
    "X = np.array(df.text)\n",
    "y = np.array(df.sentiment)\n",
    "\n",
    "train_size = int(len(df.text)*0.8)\n",
    "\n",
    "X_train = X[:train_size]\n",
    "X_test = X[train_size:]\n",
    "\n",
    "y_train = y[:train_size]\n",
    "y_test = y[train_size:]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "vocab_size = 500\n",
    "oov_tok = \"<OOV>\"\n",
    "max_length = 100\n",
    "padding = \"post\"\n",
    "truc = \"post\"\n",
    "embedding = 16"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "tokenizer = Tokenizer(num_words=vocab_size, oov_token=oov_tok)"
